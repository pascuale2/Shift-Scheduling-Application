<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shiftschedule">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".listShiftOnDay"></activity>
        <activity android:name=".viewShiftDetails" />
        <activity android:name=".createShiftWeekend" />
        <activity android:name=".createShiftWeekday" />
        <activity
            android:name=".schedule_month_view"
            android:label="@string/title_activity_schedule_month_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".changeAvailability" />
        <activity android:name=".employeeDetails" />
        <activity
            android:name=".schedule_tabs"
            android:label="@string/title_activity_schedule_tabs"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".registerpage" />
        <activity android:name=".eregisterpage" />
        <activity android:name=".landingPage" />
        <activity android:name=".employeePage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>